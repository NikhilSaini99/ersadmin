[
    {
      "name": "ers-admin-panel",
      "script": "npm",
      "args": "dev",
      "exec_mode": "cluster",
      "instances": 1,
      "watch": false,
      "merge_logs": true,
      "autorestart": true,
      "exec_interpreter": "node",
      "log_date_format": "DD-MMM-YY h:mm:ss A",
      "post_update": [
        "npm install"
      ],
      "env": {
        "NODE_ENV": "development"
      }
    }
]
